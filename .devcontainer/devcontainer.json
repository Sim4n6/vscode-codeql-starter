{
  "name": "Send RB Query",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "customizations": {
    "vscode": {
      "extensions": [
        "github.vscode-codeql",
        "slevesque.vscode-zipexplorer"
      ],
      "settings": {
        "codeQL.runningQueries.memory": 2048
      }
    }
  },
  "postCreateCommand": "git submodule init && git submodule update --recursive"
}
